{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "patch-package",
    "expo:dev": "EXPO_PACKAGER_PROXY_URL=https://$REPLIT_DEV_DOMAIN REACT_NATIVE_PACKAGER_HOSTNAME=$REPLIT_DEV_DOMAIN EXPO_PUBLIC_DOMAIN=$REPLIT_DEV_DOMAIN:5000 npx expo start --localhost",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "expo:start:static:build": "npx expo start --no-dev --minify --localhost",
    "expo:static:build": "node scripts/build.js",
    "assets:optimize:web": "python3 scripts/optimize_web_assets.py",
    "web:budget": "node scripts/check_web_budget.mjs",
    "web:build": "npx expo export --platform web --output-dir web-build && npm run web:budget",
    "e2e:web:build": "EXPO_PUBLIC_API_URL=http://127.0.0.1:5051 EXPO_PUBLIC_LYRICS_SYNC_SOURCE=playwright-ux-audit EXPO_PUBLIC_LYRICS_SYNC_PROJECT_ID=proj-ux-audit EXPO_PUBLIC_DISABLE_LEARNING=1 npx expo export --platform web --output-dir web-build && npm run web:budget",
    "e2e:ux-audit": "npm run server:build && npm run e2e:web:build && npx playwright test e2e/ux-audit.spec.ts",
    "e2e:smoke": "npm run server:build && npm run web:build && npx playwright test e2e/workflow.spec.ts e2e/pwa-smoke.spec.ts",
    "e2e:full": "npm run server:build && npm run web:build && npx playwright test e2e",
    "pwa:build": "npm run web:build",
    "icons:apple": "python3 scripts/generate_apple_icons.py",
    "icons:ai": "node scripts/generate_ai_icons_openai.mjs",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "server:prod": "NODE_ENV=production node server_dist/index.js",
    "companion:dev": "tsx companion/src/index.ts",
    "companion:parse": "tsx companion/src/parse-session-info.ts",
    "companion:sample:parse": "tsx companion/src/parse-session-info.ts companion/examples/protools-session-info.txt",
    "companion:sample:push": "tsx companion/src/push-sample.ts",
    "companion:demo": "npm run companion:sample:parse && npm run companion:sample:push",
    "companion:healthcheck": "bash scripts/companion_healthcheck.sh",
    "companion:healthcheck:ci": "bash scripts/companion_healthcheck.sh --ci",
    "companion:typecheck": "tsc -p companion/tsconfig.json --noEmit",
    "db:push": "drizzle-kit push",
    "start": "npx expo start",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test tests/**/*.test.ts"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google/generative-ai": "^0.24.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@tanstack/react-query": "^5.83.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@ungap/structured-clone": "^1.3.0",
    "@xenova/transformers": "^2.17.2",
    "babel-preset-expo": "~55.0.0",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.1",
    "expo": "^55.0.0-preview.10",
    "expo-asset": "~55.0.4",
    "expo-audio": "~55.0.5",
    "expo-av": "^16.0.8",
    "expo-blur": "~55.0.5",
    "expo-constants": "~55.0.4",
    "expo-crypto": "~55.0.5",
    "expo-font": "~55.0.3",
    "expo-glass-effect": "~55.0.5",
    "expo-haptics": "~55.0.5",
    "expo-image": "~55.0.3",
    "expo-image-picker": "~55.0.5",
    "expo-linear-gradient": "~55.0.5",
    "expo-linking": "~55.0.4",
    "expo-location": "~55.0.6",
    "expo-router": "~55.0.0-preview.7",
    "expo-splash-screen": "~55.0.5",
    "expo-status-bar": "~55.0.2",
    "expo-symbols": "~55.0.3",
    "expo-system-ui": "~55.0.5",
    "expo-web-browser": "~55.0.5",
    "express": "^5.0.1",
    "ffmpeg-static": "^5.3.0",
    "fft.js": "^4.0.4",
    "fluent-ffmpeg": "^2.1.3",
    "http-proxy-middleware": "^3.0.5",
    "openai": "^6.21.0",
    "p-limit": "^7.3.0",
    "p-retry": "^7.1.1",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.1",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-keyboard-controller": "1.20.4",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.22.0",
    "react-native-svg": "15.15.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.7.2",
    "tsx": "^4.20.6",
    "wavefile": "^11.0.0",
    "ws": "^8.18.0",
    "zod": "^3.25.76",
    "zod-validation-error": "^3.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.0",
    "@playwright/test": "^1.58.2",
    "@types/express": "^5.0.0",
    "@types/pg": "^8.16.0",
    "@types/react": "~19.2.2",
    "@types/ws": "^8.18.1",
    "babel-plugin-react-compiler": "^19.0.0-beta-e993439-20250117",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.31.0",
    "eslint-config-expo": "~55.0.0",
    "expo-module-scripts": "^55.0.2",
    "patch-package": "^8.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
